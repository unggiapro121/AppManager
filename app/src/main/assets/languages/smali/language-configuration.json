{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["{", "}"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["(", ")"],
    { "open": "\"", "close": "\"", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["(", ")"],
    ["\"", "\""],
    ["<", ">"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*\\.method",
      "end": "^\\s*\\.end method"
    }
  }
}
